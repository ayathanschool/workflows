// Enhanced API methods to support lesson plan settings
import * as api from './api';

// Get app settings from the Settings sheet
export async function getAppSettings() {
  try {
    const response = await api.callGasFunction('getAppSettings');
    return response;
  } catch (error) {
    console.error('Error fetching app settings:', error);
    throw error;
  }
}

// Export all original API functions
export * from './api';